{
  "name": "Applicant Look-Up",
  "version": "1.0",
  "content_security_policy": "default-src 'self' ",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.work.ua/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Applicant Look-Up"
  }
}